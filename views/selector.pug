extends layout

block content
  h1= title
  p Welcome to #{title}
  - var image_names = ["tomato.png","onion.png","beans.png","brinjal.png","carrot.png"]
  - for(let i=0; i<3;i++){
    -var num = image_names[Math.floor(Math.random() * 5)]
    
    img(src= "/images/"+num, alt= num, style={width: "10",height: "10"})
  -}